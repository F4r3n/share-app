{
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "core:window:allow-request-user-attention",
    "shell:allow-open",
    "shell:default",
    "updater:default"
  ]
}